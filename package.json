{
    "name": "nihonc-syntax-highlighting",
    "publisher": "NicoLang1604",
    "homepage": "https://github.com/nicoewok/nihonC-syntax",
    "displayName": "NihonC Syntax Highlighting",
    "description": "A syntax highlighter for NihonC, a Japanese alternative to C. More about NihonC here: https://github.com/nicoewok/nihonC",
    "version": "1.0.2",
    "icon": "images/icon_market.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/nicoewok/nihonC-syntax.git"
    },
    "engines": {
      "vscode": "^1.78.0"
    },
    "categories": [
      "Programming Languages"
    ],
    "contributes": {
      "languages": [{
        "id": "nihonC",
        "aliases": ["NihonC", "nihonC"],
        "extensions": [".nihonC"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/icon.png",
          "dark": "./images/icon.png"
        }
      }],
      "grammars": [{
        "language": "mac",
        "scopeName": "source.mac",
        "path": "./syntaxes/mac.tmLanguage.json"
      }]
    }
  }