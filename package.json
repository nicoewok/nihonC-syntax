{
  "name": "nihonc-syntax-highlight",
  "publisher": "LangNico",
  "displayName": "NihonC Syntax Highlight",
  "description": "A syntax highlighter for NihonC, a Japanese alternative to C. More about NihonC here: https://github.com/nicoewok/nihonC",
  "version": "1.0.4",
  "icon": "images/icon_market.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicoewok/nihonC-syntax.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "nihonc",
      "aliases": ["NihonC", "nihonc"],
      "extensions": ["nihonC"],
      "configuration": "./language-configuration.json",
      "icon": {
          "light": "./images/icon.png",
          "dark": "./images/icon.png"
        }
    }],
    "grammars": [{
      "language": "nihonc",
      "scopeName": "source.nihonc",
      "path": "./syntaxes/nihonc.tmLanguage.json"
    }]
  }
}
